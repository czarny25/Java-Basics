package _1;

public class _2 {

	public static String[] checkPermutations(String wordToCheck){
		
		String[]permutations = null;
		
		for(int i = 0; i <
				wordToCheck.length(); i++){
			
			System.out.println("---");
			
			for(int j = 0; j < wordToCheck.length()-1; j++){
				
				System.out.print(wordToCheck.charAt(j));
			}
			System.out.println();
			
		}
		
		
		return permutations;
	}
	
	public static void main(String[] args) {

		
		checkPermutations("tom");
		
	}

}
